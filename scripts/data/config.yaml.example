global:
    api-listen-addr: :5183
    timeout: 10s
    memo: Relayer
    light-cache-size: 20
    log-level: info
    ics20-memo-limit: 0
    max-receiver-size: 150
chains:
    terpnetwork:
        type: cosmos
        value:
            key-directory: /home/relayer/.relayer/.keys/120u-1
            key: relayer_key
            chain-id: 120u-1
            rpc-addr: http://127.0.0.1:23657
            account-prefix: terp
            keyring-backend: test
            gas-adjustment: 1.3
            gas-prices: 0.01uterp
            min-gas-amount: 0
            max-gas-amount: 0
            debug: false
            timeout: 20s
            block-timeout: ""
            output-format: json
            sign-mode: direct
            extra-codecs: []
            coin-type: 118
            signing-algorithm: ""
            broadcast-mode: batch
            min-loop-duration: 0s
            extension-options: []
            feegrants: null
    secretnetwork:
        type: cosmos
        value:
            key-directory: /home/relayer/.relayer/.keys/secret-1
            key: relayer_key
            chain-id: devnet-1
            rpc-addr: http://127.0.0.1:23657
            account-prefix: secret
            keyring-backend: test
            gas-adjustment: 1.3
            gas-prices: 0.1uscrt
            min-gas-amount: 0
            max-gas-amount: 0
            debug: false
            timeout: 20s
            block-timeout: ""
            output-format: json
            sign-mode: direct
            extra-codecs: []
            coin-type: 529
            signing-algorithm: ""
            broadcast-mode: batch
            min-loop-duration: 0s
            extension-options: []
            feegrants: null
paths: